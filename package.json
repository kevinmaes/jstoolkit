{
  "name": "nitro-client-scaffolding",
  "version": "1.0.0",
  "description": "Easy to fork project setup",
  "main": "app.js",
  "scripts": {
    "clean": "rimraf dist",
    "lint": "eslint src",
    "build:app": "echo => building app && node_modules/.bin/webpack",
    "build:src": "echo => building source files && babel src -d dist",
    "build:tests": "echo => building tests files && babel src/tests -d dist/tests",
    "build:all": "echo => building all && npm run build:src && npm run build:tests",
    "build": "echo => cleaning dist && npm run clean && npm run lint && npm run build:all",
    "pretest": "npm run build",
    "test": "tape dist/tests/spec_runner.js | faucet",
    "watch:src": "watch 'npm test' src",
    "watch:tests": "watch 'npm test' tests",
    "watch": "npm run watch:src & npm run watch:tests"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ThomsonReutersEikon/nitro-client-scaffolding.git"
  },
  "keywords": [
    "scaffolding",
    "boilerplate",
    "tools",
    "npm",
    "babel",
    "eslint",
    "react"
  ],
  "author": "EM Client Team",
  "license": "ISC",
  "private": true,
  "bugs": {
    "url": "https://github.com/ThomsonReutersEikon/nitro-client-scaffolding/issues"
  },
  "homepage": "https://github.com/ThomsonReutersEikon/nitro-client-scaffolding#readme",
  "devDependencies": {
    "babel-cli": "^6.6.5",
    "babel-eslint": "^6.0.0",
    "documentation": "^3.0.4",
    "eslint": "^2.5.0",
    "eslint-config-airbnb": "^6.1.0",
    "eslint-plugin-babel": "^3.1.0",
    "eslint-plugin-react": "^4.2.3",
    "faucet": "0.0.1",
    "tape": "^4.5.1",
    "tape-catch": "^1.0.4",
    "watch": "^0.17.1",
    "webpack-dev-middleware": "^1.6.1",
    "webpack-hot-middleware": "^2.10.0"
  },
  "dependencies": {
    "babel-core": "^6.7.2",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "lodash": "^4.6.1",
    "react": "^0.14.7",
    "react-dom": "^0.14.7",
    "rimraf": "^2.5.2",
    "webpack": "^1.2.0"
  }
}
